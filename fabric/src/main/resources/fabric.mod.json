{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_author}"
    ],
    "license": "${license}",
    "icon": "localizedbook_icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "me.marlester.localizedbook.fabric.LocalizedBookFabric"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": "*",
        "fabric": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=17",
        "patchouli": ">=${patchouli_version}-FABRIC"
    }
}
  